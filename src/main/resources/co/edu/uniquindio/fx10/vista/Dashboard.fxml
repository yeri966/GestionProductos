<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
                fx:controller="co.edu.uniquindio.fx10.controlador.DashboardController"
                prefHeight="400.0" prefWidth="600.0">

    <Button fx:id="btnListadoProducto"
            layoutX="50" layoutY="100"
            text="Ver Productos"
            onAction="#abrirListadoProducto"/>

    <Button fx:id="btnFormularioProducto"
            layoutX="200" layoutY="100"
            text="Agregar Producto"
            onAction="#abrirFormularioProducto"/>
</AnchorPane>